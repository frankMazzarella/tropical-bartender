{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": [
    "vue"
  ],
  "rules": {
    "no-use-before-define": "off",
    "no-console": "off",
    "max-len": ["error", { "code": 120 }]
  }
}
